#include<stdio.h>
#include<conio.h>
void main(){
clrscr();
int a[3][3],i,j,sum;
printf("input elements:\n");
for(i=0;i<3;i++){
for(j=0;j<3;j++){
printf("element-[%d][%d]:",i,j);
scanf("%d",&a[i][j]);
}
}
printf("\n the matrix is:\n");
for(i=0;i<3;i++){
printf("\n");
for(j=0;j<3;j++)
printf("%d\t",a[i][j]);
}
printf("\n\n");
for(i=0;i<3;++i){
sum=sum+a[i][i];
}
printf("\n sum of diagonal elements %d",sum);
}
